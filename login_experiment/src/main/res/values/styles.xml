<?xml version="1.0" encoding="utf-8"?>
<resources>

  <style name="formComponent">
    <item name="android:layout_marginTop">@dimen/marginNormal</item>
  </style>

  <style name="loginExperimentEditText">
    <item name="android:background">@drawable/edit_text_border</item>
    <item name="android:padding">@dimen/dimenSmall</item>
  </style>

  <style name="loginExperimentSpinner">
    <item name="android:background">@drawable/edit_text_border</item>
    <item name="android:padding">@dimen/dimenSmall</item>
  </style>

  <style name="loginExperimentButton">
    <item name="android:textAllCaps">false</item>
    <item name="backgroundTint">#2258D0</item>
  </style>

  <style name="iconButton" parent="Widget.MaterialComponents.Button.OutlinedButton">
    <item name="android:height">56dp</item>
    <item name="android:width">208dp</item>
    <item name="strokeWidth">1dp</item>
    <item name="strokeColor">#66000000</item>
    <item name="android:textAllCaps">false</item>
    <item name="android:textColor">@android:color/black</item>
  </style>

  <style name="titleStyle">
    <item name="android:textSize">30sp</item>
    <item name="android:textStyle">bold</item>
    <item name="android:textColor">@android:color/black</item>
  </style>

  <style name="TextInputLayout" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
    <item name="colorPrimary">@color/teal_200</item>
    <item name="android:textColorHighlight">@color/teal_200</item>
    <item name="android:textColor">@android:color/black</item>
    <item name="android:textColorHint">#939393</item>
  </style>
</resources>